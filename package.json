{
    "name": "sharegoods-ui",
    "version": "0.0.25",
    "description": "a solution for Admin BU",
    "scripts": {
        "bootstrap": "npm i",
        "clean": "rimraf lib",
        "dev": "npm run serve",
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "build:utils": "cross-env ENV=Utils babel src  --out-dir lib --ignore src/index.js",
        "dist": "npm run clean && npm run lib && npm run build:utils",
        "lib": "vue-cli-service build --target lib --name index --dest lib src/index.js",
        "lint": "eslint src/**/*  packages/**/* --quiet",
        "release": "sh build/release.sh"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,tsx,js,vue}": [
            "eslint",
            "git add"
        ]
    },
    "main": "./lib/index.common.js",
    "dependencies": {
        "axios": "0.18.0",
        "qs": "6.7.0",
        "resize-observer-polyfill": "1.5.1",
        "throttle-debounce": "1.0.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.4.4",
        "@babel/core": "7.0.0",
        "@babel/preset-env": "^7.4.5",
        "@babel/register": "7.0.0",
        "@vue/cli-plugin-babel": "3.7.0",
        "@vue/cli-plugin-eslint": "3.7.0",
        "@vue/cli-service": "3.7.0",
        "@vue/component-compiler-utils": "^3.0.0",
        "@vue/eslint-config-standard": "4.0.0",
        "chalk": "2.4.1",
        "chokidar": "2.1.5",
        "core-js": "2.6.5",
        "cross-env": "^5.2.0",
        "echarts": "^4.2.1",
        "element-ui": "2.8.2",
        "eslint": "^5.16.0",
        "eslint-plugin-vue": "^5.0.0",
        "highlight.js": "^9.15.8",
        "husky": "^3.0.0",
        "js-cookie": "^2.2.0",
        "lint-staged": "^9.1.0",
        "markdown-it": "^8.4.2",
        "markdown-it-anchor": "^5.2.4",
        "markdown-it-chain": "^1.3.0",
        "markdown-it-checkbox": "^1.1.0",
        "markdown-it-container": "^2.0.0",
        "mockjs": "1.0.1-beta3",
        "moment": "^2.24.0",
        "node-sass": "^4.9.0",
        "rimraf": "^2.6.3",
        "sass-loader": "^7.1.0",
        "transliteration": "^2.1.3",
        "vue": "2.5.17",
        "vue-router": "3.0.1",
        "vue-template-compiler": "2.5.17"
    },
    "peerDependencies": {
        "vue": "^2.5.17",
        "element-ui": "^2.8.2"
    },
    "keywords": [
        "sharegoods"
    ],
    "author": {
        "name": "Ma Jinhui",
        "email": "majinhui@foxmail.com"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/majinhui04/sharegoods-admin-extends/issues"
    },
    "homepage": "https://github.com/majinhui04/sharegoods-admin-extends#readme",
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 9"
    ]
}
